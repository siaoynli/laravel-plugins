{
  "name": "siaoynli/laravel-plugins",
  "description": "A powerful plugin system framework for Laravel 11",
  "type": "library",
  "keywords": [
    "laravel",
    "plugin",
    "plugin-system",
    "plugin-manager",
    "extensible"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Siaoynli",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Siaoynli\\Plugins\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Siaoynli\\Plugins\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siaoynli\\Plugins\\Providers\\PluginServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}